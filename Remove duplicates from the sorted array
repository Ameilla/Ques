#include<stdio.h>
main()
{
    int n,a[10],i,j,k,temp;
    printf("Enter the size of an array: ");

    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        scanf("%d",&a[i]);
    }

    for(i=1;i<=n;i++)
    {
        for(j=i+1;j<=n;j++)
        {
            if(a[i]>a[j])
            {
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
/*    for(i=1;i<=n;i++)
    {
        printf("%d ",a[i]);
    }

*/
    for(i=1;i<=n;i++)
    {
        for(j=i+1;j<=n;j++)
        {
            if(a[i]==a[j])
            {
                for(k=j;k<=n;k++)
                {
                    a[k]=a[k+1];
                }
                n--;
                j--;
            }
        }
    }
    for(i=1;i<=n;i++)
    {
        printf("%d ",a[i]);
    }
}

