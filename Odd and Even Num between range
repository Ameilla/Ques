#include<stdio.h>
main()
{
    int m,n,i;
    scanf("%d%d",&m,&n);

    printf("All Even Numbers= ");
    for(i=m+1;i<n;i++)
    {
        if(i%2==0)
            printf("%d ",i);
    }
    printf("\nAll Odd Numbers= ");
    for(i=m+1;i<n;i++)
    {
        if(i%2!=0)
            printf("%d ",i);
    }
}
