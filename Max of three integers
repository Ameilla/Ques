
#include<stdio.h>
main()
{
    int a[3]={1101,1011,1001};
    int i,j,temp;
    for(i=0;i<3;i++)
    {
        for(j=i+1;j<3;j++)
        {
            if(a[i]>a[j])
            {
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
    for(i=0;i<3;i++)
    {
        printf("%d ",a[i]);
    }

    printf("\nMaximum Number: %d",a[3]);
}
