
#include<stdio.h>
main()
{
    int n,i,j;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n+n;j++)
        {
            if((i==1&&j<=n+n)||(i==n&&j<=n+n)||(j==1&&i<=n)||(j==n+n&&i<=n))
                printf("* ");
            else
                printf("  ");
        }
        printf("\n");
    }

}
